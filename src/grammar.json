    },
    "_int": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "0x"
                },
                {
                  "type": "STRING",
                  "value": "0X"
                }
              ]
            },
            {
              "type": "PATTERN",
              "value": "[\\da-fA-F](_?[\\da-fA-F])*"
            }
          ]
        }
      ]
    },
    "member_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "object_identifier",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
