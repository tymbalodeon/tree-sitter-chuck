{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "chuck",
      "camelcase": "Chuck",
      "title": "Chuck",
      "scope": "source.chuck",
      "file-types": ["ck"],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^chuck$",
      "class-name": "TreeSitterChuck"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Chuck grammar for tree-sitter",
    "authors": [
      {
        "name": "Ben Rosen",
        "email": "benjamin.j.rosen@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tymbalodeon/tree-sitter-chuck"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
